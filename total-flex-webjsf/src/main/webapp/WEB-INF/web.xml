<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">



    <servlet>
        <servlet-name>UnidadeDeMedidaServlet</servlet-name>
        <servlet-class>com.passos.totalflex.webjsf.servlet.crud.UnidadeDeMedidaServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UnidadeDeMedidaServlet</servlet-name>
        <url-pattern>/servlet/UnidadeDeMedidaServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>PopulaBaseDeDados</servlet-name>
        <servlet-class>com.passos.totalflex.webjsf.servlet.util.PopulaServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PopulaBaseDeDados</servlet-name>
        <url-pattern>/servlet/populaBaseDeDado</url-pattern>
    </servlet-mapping>

    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>

</web-app>
